{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "standard"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": 13,
        "sourceType": "module",
        "ecmaFeatures": {
            "modules": true,
            "jsx": true
          }
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-unused-vars": "off"
    }
}
